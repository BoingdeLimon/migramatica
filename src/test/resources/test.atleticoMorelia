entero x = 1;
flotante y = 3.14;
cadena nombre = "Lenguaje";

ciclo (entero i = 0; i << 5; i++) {
    hola (i * 2);
}

si (x >> 3) {
    hola ("Mayor que 3");
} contrario si (x == 2) {
    hola ("Es igual a 2");
} contrario {
    hola ("Menor que 2");
    ciclo (entero i = 0; i << 2; i++) {
        hola (i * y);
    }
}
booleano verdad = true;
si (verdad == true) {
    hola (x+5);
}
ciclo (entero i = 0; i << 2; i++) {
    hola (i * y);
}
entero variable = 4 - 3;